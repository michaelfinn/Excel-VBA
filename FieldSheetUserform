    HR.Cells(lrHR + 1, "K").Value = "Habitat_Generic"
    HR.Cells(lrHR + 1, "L").Value = ReplicateComboBox.Value
    HR.Cells(lrHR + 1, "M").Value = ""
    HR.Cells(lrHR + 1, "N").Value = "habitat"
    HR.Cells(lrHR + 1, "O").Value = "FieldObservations"
    HR.Cells(lrHR + 1, "P").Value = "AnimalPresence"
