        FR.Range("A:A").SpecialCells(xlCellTypeBlanks).EntireRow.Delete
  
    
'''''This bit populates the HabitatResults Worksheet''''''

    lrHR = Sheets("HabitatResults").Range("A" & Rows.Count).End(xlUp).row

' Adds Animal Presence
    HR.Cells(lrHR + 1, "A").Value = StationCodeBox.Value
