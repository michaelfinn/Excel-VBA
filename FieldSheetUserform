Private Sub UserForm_QueryClose(Cancel As Integer, CloseMode As Integer)

If CloseMode = 0 Then
    Cancel = True
    MsgBox "Please close the form with button on the bottom.", vbCritical
End If

End Sub

Private Sub CloseButton_Click()

'closes the form window

If MsgBox("Are you sure you want to close?", vbYesNo) = vbYes Then Unload Me


End Sub

Private Sub ClearButton_Click()

'This instructs the form to reset to its blank default state

If MsgBox("Are you sure you want to clear the form?", vbYesNo) = vbYes Then UserForm_Initialize

End Sub



Private Sub AddDataButton_Click()

If MsgBox("You are about to add the entered data into the workbook. Do you wish to continue?", vbYesNo) = vbYes Then DataTransfer
    
End Sub


Private Sub DataTransfer()

'This sub instructs the form where to put all the entered values on the form
'into the spreadsheet

Dim lrLoc As Long   'Last Row Locations Worksheet
Dim lrP As Long     'Last Row Personnel Worksheet
Dim lrFC As Long    'Last Row FundingCode Worksheet
Dim lrLC As Long    'Last Row LabCollection Worksheet
Dim lrFR As Long    'Last Row FieldResults Worksheet
Dim lrHR As Long    'Last Row HabitatResults Worksheet
Dim x As Long       'Variable in Calibration Data Code



Dim Loc As Worksheet    'Location Worksheet
Dim Pers As Worksheet   'Personnel Worksheet
Dim FC As Worksheet     'FundingCode Worksheet
Dim LC As Worksheet     'LabCollection Worksheet
Dim FR As Worksheet     'FieldResults Worksheet
Dim HR As Worksheet     'HabitatResults Worksheet

Dim LabCollectionCopy


Set Loc = Sheets("Locations")
Set Pers = Sheets("Personnel")
Set FC = Sheets("FundingCode")
Set LC = Sheets("LabCollection")
Set FR = Sheets("FieldResults")
Set HR = Sheets("HabitatResults")



'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
''''''''''''''''''''''This code tells the form where to insert all the'''''''''''''''
''''''''''''''''''''''    data that's been entered into the form          '''''''''''
'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

    'This line finds the last empty row on the sheet
        'lrLoc = Sheets("Locations").Range("A" & Rows.Count).End(xlUp).Row
    
    'The following section inserts the data from the form into their
    'respective locations on the applicable worksheet, in this case
    'the "Locations" worksheet
        'Loc.Cells(lrLoc + 1, "A").Value = StationCodeBox.Value
        'Loc.Cells(lrLoc + 1, "B").Value = SampleDateBox.Value
        'Loc.Cells(lrLoc + 1, "C").Value = ProjectCodeComboBox.Value

'''''This bit populates the Locations Worksheet''''''

    lrLoc = Sheets("Locations").Range("A" & Rows.Count).End(xlUp).row
    
    Loc.Cells(lrLoc + 1, "A").Value = StationCodeBox.Value
    Loc.Cells(lrLoc + 1, "B").Value = SampleDateBox.Value
    Loc.Cells(lrLoc + 1, "C").Value = ProjectCodeComboBox.Value
    Loc.Cells(lrLoc + 1, "D").Value = EventCodeComboBox.Value
    Loc.Cells(lrLoc + 1, "E").Value = ProtocolCodeComboBox.Value
    Loc.Cells(lrLoc + 1, "F").Value = AgencyCodeComboBox.Value
    Loc.Cells(lrLoc + 1, "G").Value = SampleCommentsBox.Value
    Loc.Cells(lrLoc + 1, "H").Value = LocationComboBox.Value
    Loc.Cells(lrLoc + 1, "I").Value = GeometryShapeComboBox.Value
    Loc.Cells(lrLoc + 1, "K").Value = LatitudeBox.Value
    Loc.Cells(lrLoc + 1, "L").Value = LongitudeBox.Value
    Loc.Cells(lrLoc + 1, "M").Value = DatumComboBox.Value
    Loc.Cells(lrLoc + 1, "O").Value = ElevationBox.Value
    Loc.Cells(lrLoc + 1, "P").Value = "ft"
    Loc.Cells(lrLoc + 1, "U").Value = FailureComboBox.Value
    Loc.Cells(lrLoc + 1, "W").Value = OccupationMethodComboBox.Value
    Loc.Cells(lrLoc + 1, "X").Value = StartingBankComboBox.Value
    Loc.Cells(lrLoc + 1, "AE").Value = HydroModComboBox.Value
    Loc.Cells(lrLoc + 1, "AF").Value = HydroModLocationComboBox.Value
    Loc.Cells(lrLoc + 1, "AH").Value = GPSComboBox.Value
    Loc.Cells(lrLoc + 1, "AI").Value = GPSAccuracyBox.Value
    Loc.Cells(lrLoc + 1, "AJ").Value = "ft"
    Loc.Cells(lrLoc + 1, "AK").Value = UserInitialsBox.Value
    Loc.Cells(lrLoc + 1, "AL").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
    Loc.Cells(lrLoc + 1, "AM").Value = SampleTimeBox.Value
    
    '''''This part populates the Sample Purpose Values in Locations Worksheet''''''
    
    If HabitatCheckBox.Value = True Then Loc.Cells(lrLoc + 1, "T").Value = HabitatCheckBox.Caption
    
        If WaterChemCheckBox.Value = True Then Loc.Cells(lrLoc + 1, "T").Value = Loc.Cells(lrLoc + 1, "T").Value & ";" _
        & WaterChemCheckBox.Caption
    
        If FieldMeasureCheckBox.Value = True Then Loc.Cells(lrLoc + 1, "T").Value = Loc.Cells(lrLoc + 1, "T").Value & ";" _
        & FieldMeasureCheckBox.Caption
            
        If WaterToxCheckBox.Value = True Then Cells(lrLoc + 1, "T").Value = Loc.Cells(lrLoc + 1, "T").Value & ";" _
        & WaterToxCheckBox.Caption
    
'''''This bit populates the Personnel Worksheet''''''
    
    lrP = Sheets("Personnel").Range("A" & Rows.Count).End(xlUp).row
    
    Pers.Cells(lrP + 1, "A").Value = StationCodeBox.Value
    Pers.Cells(lrP + 1, "B").Value = SampleDateBox.Value
    Pers.Cells(lrP + 1, "C").Value = ProjectCodeComboBox.Value
    Pers.Cells(lrP + 1, "D").Value = EventCodeComboBox.Value
    Pers.Cells(lrP + 1, "E").Value = ProtocolCodeComboBox.Value
    Pers.Cells(lrP + 1, "F").Value = AgencyCodeComboBox.Value
    Pers.Cells(lrP + 1, "G").Value = SampleCommentsBox.Value
    Pers.Cells(lrP + 1, "I").Value = EventCodeComboBox.Value
    Pers.Cells(lrP + 1, "H").Value = Personnel1ComboBox.Value & ";" & Personnel2ComboBox.Value
    
'''''This bit populates the FundingCode Worksheet''''''
 
    lrFC = Sheets("FundingCode").Range("A" & Rows.Count).End(xlUp).row
    
    FC.Cells(lrFC + 1, "A").Value = StationCodeBox.Value
    FC.Cells(lrFC + 1, "B").Value = SampleDateBox.Value
    FC.Cells(lrFC + 1, "C").Value = ProjectCodeComboBox.Value
    FC.Cells(lrFC + 1, "D").Value = EventCodeComboBox.Value
    FC.Cells(lrFC + 1, "E").Value = ProtocolCodeComboBox.Value
    FC.Cells(lrFC + 1, "F").Value = AgencyCodeComboBox.Value
    FC.Cells(lrFC + 1, "G").Value = SampleCommentsBox.Value
    FC.Cells(lrFC + 1, "H").Value = FundingCodeComboBox.Value
    
'''''This bit populates the LabCollection Worksheet'''''

    lrLC = Sheets("LabCollection").Range("A" & Rows.Count).End(xlUp).row
    
    LC.Cells(lrLC + 1, "A").Value = StationCodeBox.Value
    LC.Cells(lrLC + 1, "B").Value = SampleDateBox.Value
    LC.Cells(lrLC + 1, "C").Value = ProjectCodeComboBox.Value
    LC.Cells(lrLC + 1, "D").Value = EventCodeComboBox.Value
    LC.Cells(lrLC + 1, "E").Value = ProtocolCodeComboBox.Value
    LC.Cells(lrLC + 1, "F").Value = AgencyCodeComboBox.Value
    LC.Cells(lrLC + 1, "G").Value = SampleCommentsBox.Value
    LC.Cells(lrLC + 1, "H").Value = LocationComboBox.Value
    LC.Cells(lrLC + 1, "I").Value = GeometryShapeComboBox.Value
    LC.Cells(lrLC + 1, "J").Value = SampleTimeBox.Value
    LC.Cells(lrLC + 1, "K").Value = "Water_Grab"
    LC.Cells(lrLC + 1, "L").Value = "Grab"
    LC.Cells(lrLC + 1, "M").Value = ReplicateComboBox.Value
    LC.Cells(lrLC + 1, "N").Value = CollectionDeviceComboBox.Value
    LC.Cells(lrLC + 1, "O").Value = "0.1"
    LC.Cells(lrLC + 1, "P").Value = "m"
    LC.Cells(lrLC + 1, "Q").Value = "Subsurface"
    LC.Cells(lrLC + 1, "S").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
    
    If FieldDupCheckBox.Value = True Then LC.Cells(lrLC + 1, "A").EntireRow.Copy
    If FieldDupCheckBox.Value = True Then LC.Cells(lrLC + 2, "A").Insert
    If FieldDupCheckBox.Value = True Then LC.Cells(lrLC + 2, "K").Value = "Water_Grab"
    If FieldDupCheckBox.Value = True Then LC.Cells(lrLC + 2, "L").Value = "FieldBLDup_Grab"
    
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 1, "A").EntireRow.Copy
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "A").Insert
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "K").Value = "Not Applicable"
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "L").Value = "FieldBlank"
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "N").Value = "None"
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "O").Value = "-88"
    If FieldBlankCheckBox.Value = True Then LC.Cells(lrLC + 2, "Q").Value = "Not Applicable"
        
    'If LabDupCheckBox.Value = True Then LC.Cells(lrLC + 1, "A").EntireRow.Copy
    'If LabDupCheckBox.Value = True Then LC.Cells(lrLC + 2, "A").Insert
    

'''''This bit populates the FieldResults Worksheet'''''

    lrFR = Sheets("FieldResults").Range("A" & Rows.Count).End(xlUp).row
    
   '''''''''''''''This chunk enters the 3 Water Temp Values''''''''''''
    If WaterTemp1Box.Value <> "" Then
        FR.Cells(lrFR + 1, "A").Value = StationCodeBox.Value
        FR.Cells(lrFR + 1, "B").Value = SampleDateBox.Value
        FR.Cells(lrFR + 1, "C").Value = ProjectCodeComboBox.Value
        FR.Cells(lrFR + 1, "D").Value = EventCodeComboBox.Value
        FR.Cells(lrFR + 1, "E").Value = ProtocolCodeComboBox.Value
        FR.Cells(lrFR + 1, "F").Value = AgencyCodeComboBox.Value
        FR.Cells(lrFR + 1, "G").Value = SampleCommentsBox.Value
        FR.Cells(lrFR + 1, "H").Value = LocationComboBox.Value
        FR.Cells(lrFR + 1, "I").Value = GeometryShapeComboBox.Value
        FR.Cells(lrFR + 1, "J").Value = SampleTimeBox.Value
        FR.Cells(lrFR + 1, "K").Value = "Field"
        FR.Cells(lrFR + 1, "L").Value = ReplicateComboBox.Value
        FR.Cells(lrFR + 1, "M").Value = "0.1"
        FR.Cells(lrFR + 1, "N").Value = "m"
        FR.Cells(lrFR + 1, "O").Value = "Subsurface"
        FR.Cells(lrFR + 1, "Q").Value = "samplewater"
        FR.Cells(lrFR + 1, "R").Value = "FieldMeasure"
        FR.Cells(lrFR + 1, "S").Value = "Temperature"
        FR.Cells(lrFR + 1, "T").Value = "None"
        FR.Cells(lrFR + 1, "U").Value = "Deg C"
        FR.Cells(lrFR + 1, "V").Value = "1"
        FR.Cells(lrFR + 1, "W").Value = WaterTemp1Box.Value
        FR.Cells(lrFR + 1, "X").Value = "="
        FR.Cells(lrFR + 1, "Y").Value = "None"
        FR.Cells(lrFR + 1, "AB").Value = "NA"
        FR.Cells(lrFR + 1, "AC").Value = "NR"
        FR.Cells(lrFR + 1, "AE").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
        FR.Cells(lrFR + 1, "AF").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
    End If
            
            
    If WaterTemp2Box.Value <> "" Then
        FR.Cells(lrFR + 2, "A").Value = StationCodeBox.Value
