'''''This part inputs calibration data for Cal Device 1 if check boxes were marked'''''''

    If Device1TempCheckBox.Value = True Then
        For x = 1 To 65536
            If InStr(1, FR.Range("$S$" & x), "Temperature") > 0 Then
                FR.Range("$Z$" & x) = CalDevice1ComboBox.Value
                FR.Range("$AA$" & x) = CalDate1Box.Value
            End If
        Next
    End If
    
    If Device1SCCheckBox.Value = True Then
