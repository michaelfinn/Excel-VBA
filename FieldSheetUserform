    HR.Cells(lrHR + 1, "R").Value = "none"
    HR.Cells(lrHR + 1, "S").Value = AnimalPresenceComboBox.Value
    HR.Cells(lrHR + 1, "U").Value = "="
    HR.Cells(lrHR + 1, "V").Value = "none"
    HR.Cells(lrHR + 1, "W").Value = "none"
    HR.Cells(lrHR + 1, "X").Value = "NA"
    HR.Cells(lrHR + 1, "Y").Value = "NA"
    HR.Cells(lrHR + 1, "Z").Value = ""
    HR.Cells(lrHR + 1, "AA").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
    HR.Cells(lrHR + 1, "AB").Value = LabSampleIDBox.Value & "-" & LSnumber.Value
      
'Adds Water Color
    lrHR = Sheets("HabitatResults").Range("A" & Rows.Count).End(xlUp).row
    HR.Cells(lrHR, "A").EntireRow.Copy
    HR.Cells(lrHR + 1, "A").Insert
    HR.Cells(lrHR + 1, "N").Value = "samplewater"
    HR.Cells(lrHR + 1, "P").Value = "Color"
    HR.Cells(lrHR + 1, "S").Value = WaterColorComboBox.Value
    
'Adds Dominant Substrate
    lrHR = Sheets("HabitatResults").Range("A" & Rows.Count).End(xlUp).row
    HR.Cells(lrHR, "A").EntireRow.Copy
    HR.Cells(lrHR + 1, "A").Insert
    HR.Cells(lrHR + 1, "N").Value = "habitat"
    HR.Cells(lrHR + 1, "P").Value = "DominantSubstrate"
    HR.Cells(lrHR + 1, "S").Value = DominantSubstrate1ComboBox.Value
    HR.Cells(lrHR + 1, "Z").Value = DominantSubstrate2ComboBox.Value
  
'Adds Evidence of Fire
    lrHR = Sheets("HabitatResults").Range("A" & Rows.Count).End(xlUp).row
    HR.Cells(lrHR, "A").EntireRow.Copy
    HR.Cells(lrHR + 1, "A").Insert
